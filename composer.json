{
    "name": "dominicvonk/navigation",
    "description": "Craft navigation for the website.",
    "type": "craft-plugin",
    "version": "1.0.8",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "navigation"
    ],
    "support": {
        "docs": "https://github.com/dominicvonk/navigation/blob/master/README.md",
        "issues": "https://github.com/dominicvonk/navigation/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fatfish",
            "homepage": "https://fatfish.com.au"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "dominicvonk\\navigation\\": "src/"
        }
    },
    "extra": {
        "name": "CraftNavigation",
        "handle": "craftnavigation",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://github.com/dominicvonk/navigation/master/CHANGELOG.md",
        "components": {
            "navigationService": "dominicvonk\\navigation\\services\\NavigationService"
        },
        "class": "dominicvonk\\navigation\\Navigation"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
